{
	"id": "react-basic-components",
	"name": "UXP React Basic Components",
	"version": "1.0.0",
	"main": "main.js",
	"manifestVersion": 4,
	"host": {
		"app": "PS",
		"minVersion": "22.0.0"
	},
	"entrypoints": [
		{
			"type": "panel",
			"id": "basiccomponents",
			"label": {
				"default": "UXP React Basic Components"
			},
			"minimumSize": {
				"width": 100,
				"height": 100
			},
			"maximumSize": {
				"width": 340,
				"height": 516
			},
			"preferredDockedSize": {
				"width": 340,
				"height": 516
			},
			"preferredFloatingSize": {
				"width": 340,
				"height": 315
			},
			"icons": [{
					"width": 23,
					"height": 23,
					"path": "icons/newlook_LOGO.png",
					"scale": [1, 2],
					"theme": ["dark", "darkest", "medium"],
					"species": ["chrome"]
				},
				{
					"width": 23,
					"height": 23,
					"path": "icons/newlook_LOGO.png",
					"scale": [1, 2],
					"theme": ["lightest", "light"],
					"species": ["chrome"]
				}
			]
		}
	],
	"icons": [{
			"width": 48,
			"height": 48,
			"path": "icons/newlook_LOGO.png",
			"scale": [1, 2],
			"theme": ["dark", "darkest", "medium"],
			"species": ["pluginList"]
		},
		{
			"width": 48,
			"height": 48,
			"path": "icons/newlook_LOGO.png",
			"scale": [1, 2],
			"theme": ["lightest", "light"],
			"species": ["pluginList"]
		}
	]
}
