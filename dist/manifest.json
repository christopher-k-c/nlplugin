{
	"id": "new-look-react-plugin",
	"name": "New Look Retouch Plugin",
	"version": "1.0.0",
	"main": "main.js",
	"manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "22.5.0",
      "data": {
        "apiVersion": 2
      }
    }
  ],
  "requiredPermissions": {
	"localFileSystem": "fullAccess",
    "clipboard": "readAndWrite",
    "network": { "domains": "all" },
    "allowCodeGenerationFromStrings": true,
    "ipc": {
        "enableHostCommunication": true,
        "enablePluginCommunication": true
    }
  },
	"entrypoints": [
		{
			"type": "panel",
			"id": "basiccomponents",
			"label": {
				"default": "UXP React Basic Components"
			},
			"minimumSize": {
				"width": 100,
				"height": 100
			},
			"maximumSize": {
				"width": 340,
				"height": 516
			},
			"preferredDockedSize": {
				"width": 340,
				"height": 516
			},
			"preferredFloatingSize": {
				"width": 340,
				"height": 315
			},
			"icons": [{
					"width": 23,
					"height": 23,
					"path": "icons/DarkTheme.png",
					"scale": [1, 2],
					"theme": ["dark", "darkest", "medium"],
					"species": ["chrome"]
				},
				{
					"width": 23,
					"height": 23,
					"path": "icons/LightTheme.png",
					"scale": [1, 2],
					"theme": ["lightest", "light"],
					"species": ["chrome"]
				}
			]
		}
	],
	"icons": [{
			"width": 48,
			"height": 48,
			"path": "icons/DarkTheme_plugins.png",
			"scale": [1, 2],
			"theme": ["dark", "darkest", "medium"],
			"species": ["pluginList"]
		},
		{
			"width": 48,
			"height": 48,
			"path": "icons/LightTheme_plugins.png",
			"scale": [1, 2],
			"theme": ["lightest", "light"],
			"species": ["pluginList"]
		}
	]
}
